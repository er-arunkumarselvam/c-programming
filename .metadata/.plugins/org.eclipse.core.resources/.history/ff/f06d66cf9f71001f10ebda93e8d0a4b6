 Description : Print even number to given user input
 ============================================================================
 */

#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int number;
	setbuf(stdout,NULL);
	printf("Enter a any number: ");
	scanf("%d",&number);
	for(int i=1;i<=number;i++){
		if(i/2 == 0){
			printf("%d ",i);
		}
	}
	return EXIT_SUCCESS;
}
