#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int a=1;
//	int b=++a; //preincrement
	int b=a++; //postincrement
//	printf("Pre Increment: a=%d and b=%d",a,b);
	printf("\nPost Increment: a=%d and b=%d",a,b);
	return EXIT_SUCCESS;
}
